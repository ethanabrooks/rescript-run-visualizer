{
  "name": "app",
  "version": "0.0.0",
  "license": "UNLICENSED",
  "scripts": {
    "clean": "bsb -clean-world",
    "format": "prettier -w **/*.css **/*.html **/*.json **/*.scss src/**/*.js *.js",
    "dev": "concurrently 'node server.js dev' 'yarn dev:build' 'yarn dev:bundle' 'yarn dev:css'",
    "dev:build": "bsb -make-world -w -ws _",
    "dev:bundle": "esbuild --define:NODE_GRAPHQL_ENDPOINT=\\\"$NODE_GRAPHQL_ENDPOINT\\\" --define:process.env.NODE_ENV=\\\"production\\\"  --serve=localhost:8001 --bundle src/Index.bs.js --external:react --external:react-dom",
    "dev:css": "postcss src/main.css -o dev/main.css",
    "prod": "concurrently 'yarn prod:bundle' 'yarn prod:css' 'yarn prod:static' 'yarn prod:serve'",
    "prod:bundle": "bsb -make-world && esbuild --bundle src/Index.bs.js --define:NODE_GRAPHQL_ENDPOINT=\\\"$NODE_GRAPHQL_ENDPOINT\\\" --define:process.env.NODE_ENV=\\\"production\\\" --external:react --external:react-dom --minify --outfile=dist/main.js",
    "prod:css": "NODE_ENV=production node-sass src/main.css dist/main.css",
    "prod:static": "rsync -av static/ dist/",
    "prod:serve": "node server.js dist"
  },
  "dependencies": {
    "@apollo/client": "^3.3.15",
    "@headlessui/react": "^1.0.0",
    "@heroicons/react": "^1.0.1",
    "@reasonml-community/graphql-ppx": "^1.2.0",
    "@ryyppy/rescript-promise": "^2.1.0",
    "ajv": "^8.6.0",
    "ajv-formats": "^2.1.0",
    "bs-fetch": "^0.6.2",
    "bs-platform": "^9.0.1",
    "bulma": "^0.9.2",
    "copy-to-clipboard": "^3.3.1",
    "decco": "^1.3.0",
    "get-graphql-schema": "^2.1.2",
    "glob": "^7.1.6",
    "graphql": "^15.5.0",
    "js-yaml": "^4.1.0",
    "pac": "^1.0.0",
    "prettier": "^2.2.1",
    "react-vega": "^7.4.2",
    "rescript-apollo-client": "^1.1.1",
    "subscriptions-transport-ws": "^0.9.18",
    "tailwindcss": "^2.1.2",
    "vega": "^5.20.2",
    "vega-lite": "^5.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.5",
    "concurrently": "^5.3.0",
    "cssnano": "^4.1.10",
    "esbuild": "^0.8.34",
    "express": "^4.17.1",
    "node-sass": "^6.0.0",
    "postcss": "^8.2.12",
    "postcss-cli": "^8.3.1",
    "reanalyze": "^2.16.0",
    "reason-react": "^0.9.1"
  }
}